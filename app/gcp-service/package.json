{
    "name": "@app/gcp-service",
    "private": true,
    "scripts": {
        "start": "dotenv -c production -- ts-node-dev -r tsconfig-paths/register --transpile-only ./src/index.ts",
        "dev": "dotenv -c development -- ts-node-dev --watch ./src/features -r tsconfig-paths/register --rs --transpile-only --no-notify --exit-child ./src/index.ts",
        "build": "yarn exec scripts/build.sh",
        "typecheck": "tsc -p tsconfig.prod.json --noEmit",
        "prod": "dotenv -e .env.production -- yarn node -r module-alias/register build/index.js",
        "lint": "eslint \"src/**\""
    },
    "dependencies": {
        "@fastify/cors": "^8.2.0",
        "@fastify/rate-limit": "6",
        "@google-cloud/storage": "^7.0.1",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "dotenv-cli": "5.1.0",
        "dotenv-expand": "^5.1.0",
        "fastify": "^4.10.2",
        "fastify-multer": "^2.0.3",
        "fastify-multipart": "^5.4.0",
        "form-data": "^4.0.0",
        "jsonwebtoken": "^9.0.0",
        "module-alias": "^2.2.2",
        "multer": "^1.4.5-lts.1",
        "papaparse": "^5.4.1",
        "pino-pretty": "^7.1.0",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@types/cors": "^2",
        "@types/eslint": "^7",
        "@types/jest": "^27.4.1",
        "@types/jsonwebtoken": "^8",
        "@types/module-alias": "^2.0.1",
        "@types/multer": "^1",
        "@types/node": "^20.3.1",
        "@types/papaparse": "^5",
        "@types/prettier": "^2",
        "@types/supertest": "^2.0.12",
        "@types/uuid": "^8",
        "@typescript-eslint/eslint-plugin": "^5.59.11",
        "@typescript-eslint/parser": "^5.59.11",
        "@yarnpkg/pnpify": "^3.1.1-rc.15",
        "eslint": "^8.9.0",
        "eslint-config-airbnb-typescript": "^16.1.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-import-resolver-node": "^0.3.6",
        "eslint-plugin-import": "^2.25.4",
        "jest": "^27.5.1",
        "jest-mock-extended": "^2.0.5",
        "prettier": "^2.8.0",
        "supertest": "^6.2.3",
        "ts-jest": "^27.1.4",
        "ts-node-dev": "^2.0.0",
        "tsconfig-paths": "^3.9.0",
        "typescript": "4.7.4"
    },
    "_moduleAliases": {
        "@local": "./build"
    }
}
