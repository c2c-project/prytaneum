{
    "name": "@app/server",
    "private": true,
    "scripts": {
        "lint": "eslint --ext .js,.ts . --max-warnings 10",
        "build": "tsc -p tsconfig.json",
        "start": "dotenv -c production -- ts-node --transpile-only ./src/index.ts",
        "dev": "dotenv -c development -- ts-node-dev --watch ./src/features -r tsconfig-paths/register --rs --transpile-only --no-notify --exit-child ./src/index.ts",
        "add-feat": "ts-node-dev --script-mode ./scripts/add-feature.ts",
        "typecheck": "tsc -p tsconfig.prod.json --noEmit"
    },
    "dependencies": {
        "@app/prisma": "workspace:app/prisma",
        "@google-cloud/pubsub": "^2.18.5",
        "@graphql-tools/load-files": "^6.3.2",
        "@graphql-tools/merge": "^6.2.13",
        "@graphql-tools/schema": "^7.1.3",
        "@prisma/client": "^3.3.0",
        "altair-fastify-plugin": "^4.0.2",
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "fastify": "^3.25.3",
        "fastify-cookie": "^5.3.0",
        "fastify-cors": "^6.0.2",
        "fastify-multipart": "^5.3.1",
        "form-data": "^4.0.0",
        "graphql": "^15.5.0",
        "graphql-relay": "0.9.0",
        "jsonwebtoken": "^8.5.1",
        "logstash": "^4.1.1",
        "mailgun.js": "^3.4.0",
        "mercurius": "^7.6.1",
        "mercurius-codegen": "^1.6.1",
        "module-alias": "^2.2.2",
        "mqemitter": "^4.5.0",
        "pino-pretty": "^7.1.0",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2",
        "@types/busboy": "^0.2.4",
        "@types/cors": "^2",
        "@types/eslint": "^8",
        "@types/jsonwebtoken": "^8",
        "@types/mailgun-js": "^0.22.11",
        "@types/module-alias": "^2.0.1",
        "@types/node": "^15.0.1",
        "@types/prettier": "^2",
        "@types/uuid": "^8",
        "@typescript-eslint/eslint-plugin": "^5.9.1",
        "@typescript-eslint/parser": "^5.9.1",
        "dotenv": "^8.2.0",
        "dotenv-cli": "^4.0.0",
        "dotenv-expand": "^5.1.0",
        "eslint": "^8.6.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "prettier": "^2.5.1",
        "prisma": "^3.4.0",
        "ts-node-dev": "^1.1.6",
        "tsconfig-paths": "^3.9.0",
        "typescript": "4.3.2"
    },
    "_moduleAliases": {
        "@local": "./src"
    }
}
